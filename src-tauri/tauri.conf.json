{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "JointSearch",
  "version": "0.1.0",
  "identifier": "fun.mortnon.joint",
  "build": {
    "frontendDist": "../out",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "JointSearch",
        "width": 1000,
        "height": 600,
        "resizable": true,
        "fullscreen": false,
        "minWidth": 800,
        "minHeight": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/256x256.png",
      "icons/256x256@2x.png",
      "icons/512x512.png",
      "icons/512x512@2x.png",
      "icons/icon.ico"
    ],
    "windows": {
      "nsis": {
        "installMode": "both",
        "displayLanguageSelector": true
      }
    }
  }
}
